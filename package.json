{
    "name": "data-linker",
    "version": "1.0.0",
    "main": "build/app.js",
    "repository": "git@github.com:angelxmoreno/data-linker.git",
    "author": "Angel S. Moreno <angelxmoreno@gmail.com>",
    "license": "MIT",
    "engines": {
        "node": "^14.0.0",
        "yarn": "^1.0.0"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=production TS_NODE_BASEURL=./build node -r tsconfig-paths/register build/app.js",
        "dev": "cross-env NODE_ENV=development nodemon",
        "tsc": "tsc  -p .",
        "clean": "rimraf build/*",
        "build": "yarn clean && yarn tsc",
        "lint": "yarn eslint . --ext json,js,ts --cache",
        "lint:fix": "yarn eslint . --ext json,js,ts --fix --cache",
        "docker:compose": "docker-compose --env-file .env --file ./docker/docker-compose.yml",
        "docker:start": "yarn docker:compose up",
        "docker:restart": "yarn docker:compose restart",
        "docker:stop": "yarn docker:compose stop",
        "createUser": "ts-node -T -r tsconfig-paths/register src/cli/createUser.ts",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
        "db:migration:generate": "yarn typeorm migration:generate -d ./src/database/data-source.ts -p ./src/database/migrations/diff",
        "db:migration:run": "yarn typeorm migration:run -d ./src/database/data-source.ts",
        "db:migration:revert": "yarn typeorm migration:revert -d ./src/database/data-source.ts",
        "db:schema:sync": "yarn typeorm schema:sync -d ./src/database/data-source.ts",
        "db:schema:diff": "yarn typeorm schema:log -d ./src/database/data-source.ts"
    },
    "dependencies": {
        "aws-sdk": "^2.1134.0",
        "body-parser": "^1.20.0",
        "chalk": "2.4",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "clear": "^0.1.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "debug": "^4.3.4",
        "dotenv": "^16.0.1",
        "dotenv-expand": "^8.0.3",
        "express": "^4.18.1",
        "express-winston": "^4.2.0",
        "figlet": "^1.5.2",
        "generate-password": "^1.7.0",
        "inflection": "^1.13.2",
        "inquirer": "^8.2.4",
        "lodash": "^4.17.21",
        "multer": "^1.4.4",
        "mysql2": "^2.3.3",
        "passport": "^0.5.2",
        "passport-headerapikey": "^1.2.2",
        "pg": "^8.7.3",
        "pluralize": "^8.0.0",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^4.0.0",
        "tslib": "^2.4.0",
        "typeorm": "^0.3.6",
        "typeorm-naming-strategies": "^4.1.0",
        "winston": "^3.7.2"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/clear": "^0.1.2",
        "@types/compression": "^1.7.2",
        "@types/cors": "^2.8.12",
        "@types/debug": "^4.1.7",
        "@types/express": "^4.17.13",
        "@types/express-winston": "^4.0.0",
        "@types/figlet": "^1.5.4",
        "@types/inquirer": "^8.2.1",
        "@types/lodash": "^4.14.182",
        "@types/multer": "^1.4.7",
        "@types/node": "^17.0.32",
        "@types/passport": "^1.0.7",
        "@types/pluralize": "^0.0.29",
        "@types/winston": "^2.4.4",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.0.0",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4"
    }
}
